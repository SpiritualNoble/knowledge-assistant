# 生产环境配置 - GitHub Pages
REACT_APP_APP_NAME=智能知识助手
REACT_APP_VERSION=2.0.0

# OpenAI配置 - 需要用户自行设置API密钥
REACT_APP_USE_OPENAI=true
# REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# 禁用本地AI服务（GitHub Pages无法运行）
REACT_APP_ENABLE_LOCAL_AI=false
REACT_APP_ENABLE_AI_EMBEDDING=true
REACT_APP_ENABLE_ENCRYPTION=true

# 功能配置
REACT_APP_USE_BROWSER_AI=false
REACT_APP_MODEL_PATH=/models/all-MiniLM-L6-v2-quantized.onnx
REACT_APP_TOKENIZER_PATH=/models/tokenizer.json
REACT_APP_EMBEDDING_DIMENSIONS=384

# 性能配置
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_CHUNK_SIZE=500
REACT_APP_CHUNK_OVERLAP=50
REACT_APP_BATCH_SIZE=8

# 调试配置
REACT_APP_DEBUG_MODE=false
REACT_APP_SHOW_PERFORMANCE_METRICS=true
