# 智能知识助手配置文件 - LLM-RAG版本

# 应用基本配置
REACT_APP_APP_NAME=智能知识助手
REACT_APP_VERSION=2.0.0-RAG

# LLM服务配置
# 通义千问API (推荐)
REACT_APP_QWEN_API_KEY=your_qwen_api_key_here

# OpenAI API (备用)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# API配置（可选 - 云端服务）
# REACT_APP_API_URL=https://your-worker.your-subdomain.workers.dev

# WebDAV配置（可选 - 企业网盘）
# REACT_APP_WEBDAV_URL=https://your-webdav-server.com
# REACT_APP_WEBDAV_USERNAME=your-username
# REACT_APP_WEBDAV_PASSWORD=your-password

# 功能开关
REACT_APP_ENABLE_AI_EMBEDDING=true
REACT_APP_ENABLE_ENCRYPTION=true
REACT_APP_ENABLE_WEBDAV=false
REACT_APP_ENABLE_CLOUDFLARE_API=false
REACT_APP_ENABLE_LLM_RAG=true

# AI模型配置
REACT_APP_MODEL_PATH=/models/all-MiniLM-L6-v2-quantized.onnx
REACT_APP_TOKENIZER_PATH=/models/tokenizer.json
REACT_APP_EMBEDDING_DIMENSIONS=384

# LLM配置
REACT_APP_DEFAULT_LLM_PROVIDER=qwen
REACT_APP_MAX_CONTEXT_LENGTH=4000
REACT_APP_MAX_RESPONSE_LENGTH=800

# 性能配置
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_CHUNK_SIZE=500
REACT_APP_CHUNK_OVERLAP=50
REACT_APP_BATCH_SIZE=8
REACT_APP_SEARCH_CACHE_TTL=300000

# 调试配置
REACT_APP_DEBUG_MODE=false
REACT_APP_SHOW_PERFORMANCE_METRICS=true
REACT_APP_LOG_LEVEL=info
