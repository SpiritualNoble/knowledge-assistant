# 智能知识助手前端环境变量配置

# API服务器地址
# 开发环境可以留空使用相对路径
# 生产环境需要设置为Cloudflare Workers的URL
REACT_APP_API_URL=https://your-worker.your-subdomain.workers.dev

# 应用配置
REACT_APP_APP_NAME=智能知识助手
REACT_APP_VERSION=1.0.0

# 功能开关
REACT_APP_ENABLE_EMAIL_VERIFICATION=false
REACT_APP_ENABLE_TEAM_FEATURES=false

# 文件上传配置
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.txt,.md,.ppt,.pptx

# 调试配置
REACT_APP_DEBUG_MODE=false
REACT_APP_SHOW_MOCK_DATA=true
